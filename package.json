{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "build": "next build",
    "build:backend": "cd backend && npm run build",
    "build:all": "npm run build:backend && npm run build",
    "start": "next start",
    "start:backend": "cd backend && npm run start:prod",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:generate": "cd backend && npm run prisma:generate",
    "prod:start": "pm2 start ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:restart": "pm2 restart ecosystem.config.js",
    "prod:logs": "pm2 logs",
    "prod:monitor": "pm2 monit",
    "prod:cluster": "pm2 start ecosystem.backend-cluster.config.js",
    "prod:cluster:stop": "pm2 stop ecosystem.backend-cluster.config.js",
    "prod:cluster:restart": "pm2 restart ecosystem.backend-cluster.config.js",
    "setup:prod": "bash scripts/production.sh",
    "monitor": "bash scripts/monitor.sh",
    "cleanup": "bash scripts/cleanup.sh",
    "dev:clean": "bash scripts/cleanup.sh && npm run dev:all"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-grid-layout": "^1.5.2"
  },
  "devDependencies": {
    "@nestjs/mapped-types": "^2.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-grid-layout": "^1.3.5",
    "concurrently": "^8.2.2",
    "prisma": "^6.18.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
