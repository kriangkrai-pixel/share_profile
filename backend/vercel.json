{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install && npx prisma migrate deploy && npm run build",
  "installCommand": "npm install",
  "ignoreCommand": "exit 0",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
